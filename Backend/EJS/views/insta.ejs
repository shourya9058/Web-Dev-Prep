<%- include("includes/head.ejs") %>  //used to include a subsection of code (unescaped form: bina string banae), iske liye include folder hona chahiye
  <body>
    <h2>This page belongs to @<%= data.name %></h2>
    <button>Follow</button>
    <button>Message</button>

    <p>Followers: <%= data.followers %></p>
    <p>Following: <%= data.following %></p>
    <hr />
    <% for(let post of data.posts){ %>
    <img src="<%= post.image %>" alt="some image" />
    <p>Likes: <%= post.likes %></p>
    <p>Comments: <%= post.comments %></p>

    <% } %>
  </body>
</html>
